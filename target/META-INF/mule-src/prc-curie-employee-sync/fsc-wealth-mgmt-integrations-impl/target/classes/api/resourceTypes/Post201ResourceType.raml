#%RAML 1.0 ResourceType
post:
  is: [ErrorResponses, Traceable]
  description: <<postDescription>>
  displayName: <<postDisplayName>>
  body:
    application/json:
      type: <<postEntityType>>
  responses:
    201:
      description: <<postResponseDescription>>
      body:
        application/json:
          type: <<postResponseDatatype>>
