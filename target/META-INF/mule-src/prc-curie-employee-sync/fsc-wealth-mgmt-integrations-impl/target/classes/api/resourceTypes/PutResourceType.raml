#%RAML 1.0 ResourceType
put:
  is: [ErrorResponses, Traceable]
  description: <<putDescription>>
  displayName: <<putDisplayName>>
  body:
    application/json:
      type: <<putEntityType>>
  responses:
    200:
      description: <<putResponseDescription>>
      body:
        application/json:
          type: <<putResponseDatatype>>