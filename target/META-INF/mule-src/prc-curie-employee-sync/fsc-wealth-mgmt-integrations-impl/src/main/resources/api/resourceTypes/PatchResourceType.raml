#%RAML 1.0 ResourceType
patch:
  is: [ErrorResponses]
  description: <<patchDescription>>
  displayName: <<patchDisplayName>>
  body:
    application/json:
      type: <<patchEntityType>>
  responses:
    200:
      description: <<patchResponseDescription>>
      body:
        application/json:
          type: <<patchResponseDatatype>>